* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
@font-face {
    font-family: 'NEXON Lv1 Gothic OTF';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv1 Gothic OTF.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
ul li {
    list-style: none;
}
a {
    text-decoration: none;
    color: inherit;
}
body{
    overflow-x: hidden;
}
.hide {
    display: none;
}
/* index */
.main {
    width: 100%;
    height: 899px;
}
header {
    width: 100%;
    height: 130px;
    background-color: #000;
    position: absolute;
    top: 0px;
    font-family: '넥슨Lv1고딕';
    line-height: 30px;
    z-index: 1;
}
header .logo img{
    width: 106px;
    height: 73px;
    margin-top: 30px;
    margin-left: 4.218%;
}
header .nav {
    margin-top: -60px;
}
header .nav .main-menu {
    float: left;
    color: #fff;
    margin-right: 3.02%;
    font-size: 20px;
}
header .nav .main-menu:nth-of-type(1) {
    margin-left: 33.437%;
    width: 80px;
}
header .nav .main-menu:nth-of-type(2) {
    width: 200px;
}
header .nav .main-menu:nth-of-type(3) {
    width: 120px;
    margin-right: 0px;
}
header .nav .main-menu:nth-of-type(4) {
    margin-right: 17.354%;
}
header .nav .main-menu img {
    width: 25px;
    height: 25px;
    margin-left: -30px;
}
header .nav .main-menu a:hover{
    color: rgb(58, 156, 143);
    transition: all .3s;
}
header .nav .main-menu .sub-menu {
    padding-top: 100px;
    width: 100%;
    height: 300px;
}
header .nav .main-menu .sub-menu li {
    padding-bottom: 50px;
}
header .nav .main-menu .sub-menu li a:hover{
    color: rgb(58, 156, 143);
}

header .nav:hover {
    width: 100%;
    height: 400px;
    background-color: #000;
    transition: all .5s;
}
/* login start */

.log {
    width: 100%;
    height: 950px;
    background-color: #fff;
    background: url(../img/loginbg.png) no-repeat center;
    background-size: cover;
}
.log table tr td{ 
    color: #fff;
    font-size: 20px;
}
.log table tr td h1{
    width: 100%;
