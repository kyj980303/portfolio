* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}
@font-face {
    font-family: 'NEXON Lv1 Gothic OTF';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_20-04@2.1/NEXON Lv1 Gothic OTF.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'Cafe24Oneprettynight';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_twelve@1.1/Cafe24Oneprettynight.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'GowunBatang-Regular';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2108@1.1/GowunBatang-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'GmarketSansMedium';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2001@1.1/GmarketSansMedium.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@keyframes fadeIn {
    0% {opacity: 0;}
    100% {opacity: 1;}
}
.fadeIn {
    animation: fadeIn 5s backwards;
}
.fadeIn1 {
    animation: fadeIn 6s backwards;
}
.fadeIn2 {
    animation: fadeIn 5s backwards;
    animation-delay: 1s;
}
.fadeIn22 {
    animation: fadeIn 5s backwards;
    animation-delay: 2s;
}
.fadeIn3 {
    animation: fadeIn 3s backwards;
    animation-delay: .5s;
}
.fadeIn33 {
    animation: fadeIn 3s backwards;
}
.fadeIn4 {
    animation: fadeIn 3s backwards;
}
@keyframes slideDown {
    0% {opacity:0;transform:translateY(-55px);}
    100% {opacity:1;transform:translateY(0px);}
}
@keyframes slideDown2 {
    0% {opacity:0;transform:translateY(-15px);}
    100% {opacity:1;transform:translateY(0px);}
}
.slideDown0 {
    animation: slideDown2 1s backwards;
}
.slideDown {
    animation: slideDown 2s backwards;
    animation-delay: 1s;
}
.slideDown1 {
    animation: slideDown 2s backwards;
    animation-delay: 2s;
}
.slideDown2 {
    animation: slideDown 2s backwards;
    animation-delay: 3s;
}
@keyframes slideRight1 {
    0% {opacity:1;transform:translateX(-130px);}
    100% {opacity:1;transform:translateX(0px);}
}
.slideRight1 {
    animation: slideRight1 1s backwards;
    animation-delay: 3s;
}
@keyframes slideRight2 {
    0% {opacity:1;transform:translateX(-600px);}
    100% {opacity:1;transform:translateX(0px);}
}
.slideRight2 {
    animation: slideRight2 2s backwards;
}
@keyframes slideleft1 {
    0% {opacity:1;transform:translateX(130px);}
    100% {opacity:1;transform:translateX(0px);}
}
.slideleft1 {
    animation: slideleft1 1s backwards;
    animation-delay: 3s;
}


